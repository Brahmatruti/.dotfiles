    "html.format.indentInnerHtml": true,
    "html.format.wrapLineLength": 120,
    "html.format.wrapAttributes": "auto",
    "html.format.templating": true,
    "css.format.enable": true,
    "css.format.newlineBetweenSelectors": true,
    "css.format.newlineBetweenRules": true,
    "css.format.spaceAroundSelectorSeparator": true,
    "css.format.preserveNewLines": true,
    "css.validate": true,
    "css.lint.duplicateProperties": "warning",
    "css.lint.float": "warning",
    "css.lint.idSelector": "warning",
    "css.lint.important": "warning",
    "css.lint.importStatement": "warning",
    "css.lint.boxModel": "warning",
    "css.lint.universalSelector": "warning",
    "css.lint.zeroUnits": "warning",
    "css.lint.fontFaceProperties": "warning",
    "css.lint.hexColorLength": "warning",
    "css.lint.argumentsInColorFunction": "warning",
    "css.lint.unknownProperties": "warning",
    "css.lint.ieHack": "warning",
    "css.lint.unknownVendorSpecificProperties": "warning",
    "css.lint.propertyIgnoredDueToDisplay": "warning",
    "css.lint.avoidImportant": "warning",
    "css.lint.removeEmptyRulesets": "warning",
    "css.lint.imports": "warning",
    "css.lint.cssModules": "warning",
    "css.lint.duplicateStylesheets": "warning",
    "less.format.enable": true,
    "less.format.newlineBetweenRules": true,
    "less.format.preserveNewLines": true,
    "less.validate": true,
    "scss.format.enable": true,
    "scss.format.newlineBetweenRules": true,
    "scss.format.preserveNewLines": true,
    "scss.validate": true,

